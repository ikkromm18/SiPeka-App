{
  "expo": {
    "name": "SiPeka-App",
    "slug": "SiPeka-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/sipeka-logo.png",
    "scheme": "sipekaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SiPekaApp"
    },
    "android": {
      "package": "com.anonymous.SiPekaApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Logo.png",
        "backgroundColor": "#18353D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#18353D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "81119553-c93a-4887-893c-cc771708b59b"
      }
    }
  }
}
